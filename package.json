{
  "name": "frontend-boilerplate-vite",
  "version": "1.0.0",
  "repository": "git@github.com:userName/frontend-boilerplate-vite.git",
  "license": "MIT",
  "browserslist": [
    "ie >= 11",
    ">0.2% in JP",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "postinstall": "yarn husky:init",
    "husky:init": "rimraf .config && husky install .config/husky && husky add .config/husky/pre-commit \"yarn lint-staged\"",
    "dev": "concurrently yarn:dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "build": "yarn build:vite && yarn build:eleventy ",
    "build:eleventy": "cross-env NODE_ENV=production eleventy",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "preview": "yarn build && serve --ssl-cert key/localhost.pem --ssl-key key/localhost-key.pem out",
    "lint": "yarn lint:script && yarn lint:style && yarn prettier",
    "lint:style": "stylelint --fix 'src/styles/**/*.scss'",
    "lint:script": "eslint --fix --ext .ts,.tsx --ignore-pattern .eslintignore .",
    "prettier": "yarn prettier:eslint && yarn prettier:js && yarn prettier:json",
    "prettier:eslint": "prettier --write .eslintrc.js",
    "prettier:js": "prettier --write *.js && prettier --write **/*.js",
    "prettier:json": "prettier --write *.json && prettier --write **/*.json"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint:script",
      "git add --force"
    ],
    "**/*.scss": [
      "yarn lint:style",
      "git add --force"
    ],
    "**/*.{js,json}": [
      "yarn prettier",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "7.17.2",
    "@rollup/plugin-babel": "5.3.1",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "browserslist": "4.19.3",
    "concurrently": "7.0.0",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "css-mqpacker": "7.0.0",
    "cssnano": "5.0.16",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.3",
    "postcss": "8.4.6",
    "postcss-calc": "8.2.4",
    "postcss-discard-duplicates": "5.0.2",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.3.1",
    "postcss-scss": "4.0.3",
    "postcss-sorting": "7.0.1",
    "prettier": "2.5.1",
    "sass": "1.49.0",
    "serve": "13.0.2",
    "stylelint": "14.3.0",
    "stylelint-config": "0.1.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.1.0",
    "typescript": "4.4.4",
    "vite": "2.7.2"
  }
}
